<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="models.ItemsEntity">
        <table name="ITEMS" schema="C##IMS" catalog=""/>
        <attributes>
            <id name="itemId">
                <column name="ITEM_ID" precision="10"/>
            </id>
            <basic name="itemCode">
                <column name="ITEM_CODE" length="50"/>
            </basic>
            <basic name="itemName">
                <column name="ITEM_NAME" length="100"/>
            </basic>
            <basic name="purchaseDescription">
                <column name="PURCHASE_DESCRIPTION" nullable="false" length="100"/>
            </basic>
            <basic name="isPrintBarcode">
                <column name="IS_PRINT_BARCODE" nullable="false" length="1"/>
            </basic>
            <basic name="departmentId">
                <column name="DEPARTMENT_ID" nullable="false" precision="10"/>
            </basic>
            <basic name="categoryId">
                <column name="CATEGORY_ID" nullable="false" precision="10"/>
            </basic>
            <basic name="brandId">
                <column name="BRAND_ID" nullable="false" precision="10"/>
            </basic>
            <basic name="itemTypeId">
                <column name="ITEM_TYPE_ID" nullable="false" length="50"/>
            </basic>
            <basic name="itemClassId">
                <column name="ITEM_CLASS_ID" nullable="false" length="50"/>
            </basic>
            <basic name="itemLocation">
                <column name="ITEM_LOCATION" nullable="false" length="50"/>
            </basic>
            <basic name="storeId">
                <column name="STORE_ID" nullable="false" precision="10"/>
            </basic>
            <basic name="article">
                <column name="ARTICLE" nullable="false" length="50"/>
            </basic>
            <basic name="stockQty">
                <column name="STOCK_QTY" precision="-1"/>
            </basic>
            <basic name="isTrackInventory">
                <column name="IS_TRACK_INVENTORY" nullable="false" length="1"/>
            </basic>
            <basic name="uom">
                <column name="UOM" nullable="false" length="50"/>
            </basic>
            <basic name="lowStockQty">
                <column name="LOW_STOCK_QTY" nullable="false" precision="-1"/>
            </basic>
            <basic name="minOrderQty">
                <column name="MIN_ORDER_QTY" nullable="false" precision="-1"/>
            </basic>
            <basic name="maxOrderQty">
                <column name="MAX_ORDER_QTY" nullable="false" precision="-1"/>
            </basic>
            <basic name="leadTimeDays">
                <column name="LEAD_TIME_DAYS" nullable="false" precision="-1"/>
            </basic>
            <basic name="supplierId">
                <column name="SUPPLIER_ID" nullable="false" precision="10"/>
            </basic>
            <basic name="itemOrigin">
                <column name="ITEM_ORIGIN" nullable="false" length="50"/>
            </basic>
            <basic name="openingBalance">
                <column name="OPENING_BALANCE" nullable="false" precision="-1"/>
            </basic>
            <basic name="isActive">
                <column name="IS_ACTIVE" nullable="false" length="1"/>
            </basic>
            <basic name="createdBy">
                <column name="CREATED_BY" nullable="false" precision="10"/>
            </basic>
            <basic name="createdOn">
                <column name="CREATED_ON" nullable="false" precision="6"/>
            </basic>
            <basic name="lastUpdatedBy">
                <column name="LAST_UPDATED_BY" nullable="false" precision="10"/>
            </basic>
            <basic name="lastUpdatedOn">
                <column name="LAST_UPDATED_ON" nullable="false" precision="6"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
